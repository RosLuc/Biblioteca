<AnchorPane id="AnchorPane" prefHeight="540.0" prefWidth="960.0" styleClass="background" stylesheets="@../CSS/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
        <TextField fx:id="userTxt" layoutX="337.0" layoutY="221.0" prefHeight="39.0" prefWidth="287.0" promptText="USUÁRIO" />
        <PasswordField fx:id="passTxt" layoutX="337.0" layoutY="277.0" onKeyPressed="#loginBtnKeyAction" prefHeight="39.0" prefWidth="287.0" promptText="SENHA" />
        <Button fx:id="loginBtn" layoutX="337.0" layoutY="330.0" mnemonicParsing="false" onAction="#loginBtnAction" prefHeight="39.0" prefWidth="287.0" styleClass="login-btn" text="LOGIN" />
        <ImageView fitHeight="125.0" fitWidth="419.0" layoutX="303.0" layoutY="52.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/LogoLP.png" />
            </image>
        </ImageView>
        <Button fx:id="forgotpassBtn" layoutX="371.0" layoutY="390.0" mnemonicParsing="false" onAction="#forgotPassBtnAction" prefHeight="32.0" prefWidth="220.0" styleClass="other-btn" text="ESQUECI MINHA SENHA" />
        <Button fx:id="signupBtn" layoutX="419.0" layoutY="434.0" mnemonicParsing="false" onAction="#signUpBtnAction" styleClass="other-btn" text="CADASTRAR" />
    </children>
</AnchorPane>
