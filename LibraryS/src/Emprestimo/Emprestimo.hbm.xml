<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Emprestimo.Emprestimo" table="emprestimo">
        <id name="codemp" column="codemp" type="int">
            <generator class="sequence">              
            <param name="sequence">emprestimo_codemp_seq</param>
            </generator>
        </id>
        <property name="codinsc"/>
        <property name="usercode"/>
        <property name="dataemp" type="date">
            <column name="dataemp" length="13" not-null="true" />
        </property>
        <property name="datadev" type="date">
            <column name="datadev" length="13" not-null="true" />
        </property>
        <property name="status" type="string">
            <column name="status" length="20" not-null="true" />
        </property>
        <set name="materiais" table="indicar" lazy="false" fetch="join">
            <key>
                <column name="codemp" not-null="true" />
            </key>
            <many-to-many column = "nchamada" class="Classes.Material"/>
        </set>
    </class>
</hibernate-mapping>
